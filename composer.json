{
  "name": "remzikocak/laravel-vallet",
  "description": "Vallet Payment Gateway for Laravel",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "vallet",
    "payment",
    "laravel-vallet"
  ],
  "authors": [
    {
      "name": "Remzi Kocak",
      "email": "hey@remzikocak.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.2"
  },
  "require-dev": {
    "mockery/mockery": "^1.5",
    "pestphp/pest": "^1.22",
    "phpunit/phpunit": "^9.5.24",
    "orchestra/testbench": "^7.0|^8.0"
  },
  "autoload": {
    "psr-4": {
      "RKocak\\Vallet\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RKocak\\Vallet\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "RKocak\\Vallet\\ValletServiceProvider"
      ],
      "aliases": {
        "Options": "RKocak\\Vallet\\Facades\\Payment"
      }
    }
  }
}