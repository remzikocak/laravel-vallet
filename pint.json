{
  "preset": "laravel",
  "rules": {
    "no_multiline_whitespace_around_double_arrow": false,
    "binary_operator_spaces": {
      "default": "align_single_space_minimal"
    },
    "single_import_per_statement": false,
    "no_unused_imports": true,
    "group_import": true,
    "declare_strict_types": true
  }
}